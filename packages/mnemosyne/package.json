  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/soteria/issues"
  },
  "homepage": "https://github.com/sciactive/soteria#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.4",
    "@types/jmuxer": "^2.0.6",
    "@types/update-notifier": "^6.0.5",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "jmuxer": "^2.0.5",
    "libv4l2-ts": "^1.0.1",
