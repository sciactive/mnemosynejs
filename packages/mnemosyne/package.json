  "description": "Takes a video feed and uploads it in chunks to a WebDAV server.",
  "type": "module",
  "bin": {
    "mnemosyne": "mnemosyne.cjs"
  },
  "scripts": {
    "clean": "test -d bin && rm -r bin || true",
